<!DOCTYPE html>
<html>
<head>
  <%= render_meta %>
  <%= render_title %>
  <%= stylesheet_link_tag 'application', :media=>'all' %>
  <%= stylesheet_link_tag 'application-print', :media=>'print' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= render_header %>
  <%= render_menu %>
  <div class="content">
    <%= yield %>
  </div>
</body>
</html>
