<%= fields_for "customer[customer_names_attributes]", cn, :index => index do |f| %>
<tr>
  <td>
    <%= f.hidden_field :id %>
    <%= f.hidden_field :_destroy %>
    <%= f.hidden_field :customer_id %>
    <%= f.text_field :formal_name %>
  </td>
  <td><%= f.text_field :name %></td>
  <td><%= f.date_select :start_date %></td>
  <td><%= link_to_function '削除', 'customers.remove_customer_name(this)' %></td>
</tr>
<% end %>
