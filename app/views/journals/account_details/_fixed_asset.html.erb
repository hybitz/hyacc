<%
  prefix = "journal[journal_details_attributes[#{index}]]"
%>
<% if jd.asset and jd.asset.persisted? %>
  <%= fields_for prefix, jd do |f| %>
    <%= f.fields_for 'asset_attributes', jd.asset do |f| %>
      資産コード：<%= jd.asset.code %>

      <%= f.hidden_field :id %>
      <%= f.hidden_field :lock_version %>
    <% end %>
  <% end %>
<% end %>
