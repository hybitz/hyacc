<h1>New bank_office</h1>

<%= form_for(@bank_office) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :bank_id %><br />
    <%= f.text_field :bank_id %>
  </p>
  
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  
  <p>
    <%= f.label :deleted %><br />
    <%= f.check_box :deleted %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', bank_offices_path %>