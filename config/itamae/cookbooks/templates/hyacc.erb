PATH=<%= @path %>
RAILS_ENV=<%= @rails_env %>

0 0 1 6 * <%= @user %> /bin/bash -c 'cd <%= @hyacc_root %> && bundle exec rake hyacc:notification:generate >> <%= @hyacc_root %>/log/cron.log 2>&1'
0 0 1 9 * <%= @user %> /bin/bash -c 'cd <%= @hyacc_root %> && bundle exec rake hyacc:notification:cleanup >> <%= @hyacc_root %>/log/cron.log 2>&1'
