<%= fields_for "customer[customer_names_attributes][#{index}]", cn do |f| %>
<tr>
  <td>
    <%= f.hidden_field :id %>
    <%= f.hidden_field :_destroy %>
    <%= f.text_field :formal_name %>
  </td>
  <td><%= f.text_field :name %></td>
  <td><%= f.date_select :start_date %></td>
  <td><%= link_to '削除', '#', :onclick => 'customers.remove_customer_name(this); return false;' %></td>
</tr>
<% end %>
