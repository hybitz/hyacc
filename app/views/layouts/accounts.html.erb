<!DOCTYPE html>
<html>
<head>
  <%= render_meta %>
  <%= render_title %>
  <%= stylesheet_link_tag 'application' %>
  
  <style type="text/css">
  .yui-skin-sam .yui-dt-body { cursor:pointer; } /* when rows are selectable */
  </style>

  <%= javascript_include_tag 'application' %>
  <%= render 'common/jquery_config' %>
  <%= render 'common/common_js' %>
  <%= csrf_meta_tag %>
</head>
<body class="yui-skin-sam">
  <%= render_header %>
  <%= render_menu %>
  <%= render_action_menu :actions=>['index', 'list_tree'], :titles=>['一覧表示', 'ツリー表示'] %>

  <div style="margin: 10px;">
    <%= yield %>
  </div>
</body>
</html>
