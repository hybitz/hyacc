<%= fields_for "profile[simple_slip_settings_attributes]", sss, :index => index do |f| %>
  <tr>
    <td>
        <%= f.hidden_field :id %>
        <%= f.select :account_id, to_options(@accounts), {}, class: 'form-control' %>
    </td>
    <td><%= f.text_field :shortcut_key, maxlength: 6, class: 'form-control text-center' %></td>
    <td class="text-center"><%= f.check_box '_destroy' %></td>
  </tr>
<% end %>
