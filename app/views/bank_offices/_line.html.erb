<%= fields_for "bank[bank_offices_attributes][]", bank_office, :index=>index do |f| %>
  <tr>
    <td class="bank_office_code">
      <% if bank_office.new_record? %>
        <%= f.text_field :code %>
      <% else %>
        <%= f.hidden_field :id %>
        <%= f.hidden_field :code %>
        <%= bank_office.code %>
      <% end %>
    </td>
    <td><%= f.text_field :name %></td>
    <td><%= f.select :deleted, @deleted_types %></td>
  </tr>
<% end %>

